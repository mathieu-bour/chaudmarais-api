{
    "name": "mathieu-bour/chaumarais-api",
    "description": "The Chaud Marais official API.",
    "version": "1.0.0",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "barryvdh/laravel-cors": "^0.11.3",
        "laravel/lumen-framework": "5.8.*",
        "mathrix/lumen-jwt-auth": "1.2.0",
        "mathrix/lumen-zero": "2.3.1",
        "mathrix/openapi-assertions": "0.9.1",
        "mathrix/openapi-processor": "0.9.6",
        "stripe/stripe-php": "^6.43"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "mathrix/lumen-jwt-auth": {
            "type": "vcs",
            "url": "git@github.com:mathrix-education/lumen-jwt-auth.git"
        },
        "mathrix/lumen-zero": {
            "type": "vcs",
            "url": "git@github.com:mathrix-education/lumen-zero.git"
        },
        "mathrix/openapi-processor": {
            "type": "vcs",
            "url": "git@github.com:mathrix-education/openapi-processor.git"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
